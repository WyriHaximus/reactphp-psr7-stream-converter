{
    "name": "wyrihaximus/react-psr7-stream-converter",
    "description": "PSR7 to ReactPHP stream converter and vice versa",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "psr/http-message": "^1.0",
        "react/stream": "^0.4",
        "react/event-loop": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "vectorface/dunit": "~2",
        "squizlabs/php_codesniffer": "~1.5.6",
        "phake/phake": "~1.0.6"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\PSR7StreamConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\PSR7StreamConverter\\": "tests/"
        }
    }
}
